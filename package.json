{
  "name": "Soundboard",
  "version": "0.0.1",
  "description": "tool for teachers",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds/mac",
    "package-win": "electron-packager . --overwrite --asar=true --prune=true --out=release-builds/pc",
    "package-linux": "electron-packager . --overwrite --asar=false --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package-all": "yarn package-mac && yarn package-win"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Soundboard",
    "teaching"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.8",
    "electron-packager": "^14.0.2"
  },
  "dependencies": {
    "dialogs": "^2.0.1",
    "electron-json-storage": "^4.1.6",
    "electron-positioner": "^4.1.0",
    "iohook": "^0.5.1",
    "webrtc-adapter": "^4.2.2"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "1.4.7"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-70"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
